{
  "name": "alessandro-alfano-cv",
  "version": "2.0.0",
  "description": "Alessandro Alfano's CV/Portfolio with dual retro themes",
  "scripts": {
    "dev": "eleventy --serve --port=3000",
    "build:site": "eleventy",
    "build:pdf": "markdown-pdf _site/cv-classic.md -o _site/cv-classic.pdf && markdown-pdf _site/cv-europass.md -o _site/cv-europass.pdf",
    "build": "npm run build:site && npm run build:pdf",
    "clean": "rm -rf _site"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.0.0"
  },
  "devDependencies": {
    "markdown-pdf": "^11.0.0"
  },
  "author": "Alessandro Alfano",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lysandre995/alessandro-alfano.git"
  },
  "homepage": "https://lysandre995.github.io/alessandro-alfano/"
}
